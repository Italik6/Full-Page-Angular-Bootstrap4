{
    "sass.enabled": false,
    "sass.compiler": "libsass",
    "path": {
        "scss/*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "includePaths": [],
                "outputDir": "../css/",
                "imagePath": null,
                "sourceComments": false,
                "outputStyle": "nested"
            },
            "linting.collapsed": true,
            "brackets-bower.show": true,
            "brackets-bower.reloadRegistryTime": 10,
            "brackets-bower.quickInstallSavePackages": true,
            "brackets-git.gitPath": "git",
            "themes.theme": "dark-theme"
        }
    }
}